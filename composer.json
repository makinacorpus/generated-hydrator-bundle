{
    "name": "makinacorpus/generated-hydrator-bundle",
    "description": "Provide integration of ocramius/generated-hydrator for Symfony",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Pierre Rineau",
        "email": "pierre.rineau@makina-corpus.com"
    }],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.2",
        "ocramius/generated-hydrator": "^4.5"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpunit/phpunit": "^10.0",
        "ramsey/uuid": "^4.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": ">=6.0",
        "symfony/filesystem": ">=6.0",
        "symfony/http-kernel": ">=6.0",
        "symfony/property-info": ">=6.0",
        "symfony/yaml": ">=6.0"
    },
    "autoload": {
        "psr-4": {
            "GeneratedHydrator\\Bridge\\Symfony\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GeneratedHydrator\\Bridge\\Symfony\\Tests\\": "tests/"
        }
    }
}
